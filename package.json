{
  "name": "wx-formid-collector",
  "version": "0.0.1",
  "description": "Form ID 收集组件",
  "author": "YY YiJian Team",
  "license": "MIT",
  "private": true,
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "git add"
    ]
  },
  "scripts": {
    "precommit": "lint-staged",
    "eslint": "eslint --config .eslintrc.js --fix src/**/*.{js,wxs}",
    "lint": "npm run eslint",
    "development": "wxparcel-script start --env dev --watch",
    "production": "wxparcel-script start --env prod --publicPath local",
    "test": "npm run lint",
    "start": "npm run development"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^4.14.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "wxparcel-script": "^1.0.0-alpha.10.0"
  }
}
